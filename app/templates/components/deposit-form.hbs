<fieldset>
  <legend>{{title}}</legend>
  {{date-field value=model.date}}
  {{date-field title="Last Meeting Date" value=model.forLastMeeting}}
  {{money-field title="Cash" value=model.cash}}
  {{money-field title="Cheques" value=model.cheques}}
  {{total-field value=total}}
  <div class="submit">
    <button type="submit">{{submitText}}</button>
  </div>
</fieldset>
