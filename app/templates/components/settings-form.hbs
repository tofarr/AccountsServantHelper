<fieldset>
  <legend>{{title}}</legend>
  {{money-field title="Opening Balance" value=model.openingBalance}}
  {{money-field title="Other Balance" value=model.otherBalance}}
  {{money-field title="Default Outgoing Cheque Amount" value=model.defaultOutgoingChequeAmt}}
  {{text-field title="Congregation" value=model.congregation}}
  {{text-field title="City" value=model.city}}
  {{text-field title="State" value=model.state}}
  {{text-field title="Accounts Servant" value=model.accountsServantName}}
  {{day-field title="Midweek Meeting Day" value=model.midweekMeetingDay}}
  {{day-field title="Watchtower Meeting Day" value=model.watchtowerMeetingDay}}
  {{money-field title="KHAHC" value=model.khahc}}
  {{money-field title="GAA" value=model.gaa}}
  {{money-field title="COAA" value=model.coaa}}
  {{money-field title="CT" value=model.ct}}
  {{text-field title="Dropbox Token" value=model.dropboxToken}}
  {{text-field title="Last Sync" value=model.dropboxSyncedAt disabled=true}}
  <div class="submit">
    <button type="submit">{{submitText}}</button>
  </div>
</fieldset>
