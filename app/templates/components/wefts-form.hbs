<fieldset>
  <legend>{{title}}</legend>
  {{date-field value=model.date}}
  {{date-field title="Last Meeting Date" value=model.forLastMeeting}}
  {{text-field title="Transfer Id" value=model.transferId}}
  {{money-field title="KHAHC" value=model.khahc}}
  {{money-field title="GAA" value=model.gaa}}
  {{money-field title="COAA" value=model.coaa}}
  {{money-field title="CT" value=model.ct}}
  {{money-field title="Worldwide" value=model.worldwide}}
  {{total-field value=total}}

  <div class="submit">
    <button type="submit">{{submitText}}</button>
  </div>
</fieldset>
