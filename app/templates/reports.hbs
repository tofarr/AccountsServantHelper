<table class="crud-table reports-table" cellpadding="0" cellspacing="0">
  <thead>
    <tr>
      <th>Date</th>
      <th>&nbsp;</th>
    </tr>
  </thead>
  <tbody>
    {{#each model.months as |month|}}
      <tr>
        <td>{{month}}</td>
        <td class="actions">
          {{#link-to "statement" (query-params month=month)}}<button>Statement</button>{{/link-to}}
          {{#link-to "s26" (query-params month=month)}}<button>S26</button>{{/link-to}}
          {{#link-to "s30" (query-params month=month)}}<button>S30</button>{{/link-to}}
        </td>
      </tr>
    {{/each}}
</tbody>
</table>
